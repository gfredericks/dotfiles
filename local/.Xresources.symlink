
! Effectuating this file immediately after saving may require
! running xrdb -merge .Xresources

! Inconsolata does not seem to work with unicode (?)
XTerm*faceName: DejaVu Sans Mono
XTerm*faceSize: 16
XTerm*faceSize1: 8
XTerm*faceSize2: 10
XTerm*faceSize3: 12
XTerm*faceSize4: 16
XTerm*faceSize5: 20
XTerm*faceSize6: 32
XTerm*utf8: 2
XTerm*dynamicColors: true

! Gets the Alt key working
XTerm*metaSendsEscape: true
XTerm.fullscreen: never


*customization:     -color
! I am commenting this out, which I think effectively disables
! all the extra colors defined below, since the dynamic-colors+solarized
! thing that I use to switch between light and dark mode only
! involves 8/16/whatever colors, and the rest of these colors
! mess it up; if this doesn't work well then I'll probably
! need to find a pair of 256-color themes as an alternative
! to solarized
! XTerm*termName: xterm-256color

xterm*VT100.Translations: #override \
                       Shift <Key>Up:   scroll-back(1) \n\
                       Shift <Key>Down: scroll-forw(1) \n\
                 Ctrl  Shift <Key>V:    insert-selection(CLIPBOARD) \n\
                 Ctrl  Shift <Key>C:    copy-selection(CLIPBOARD) \n\
                 Ctrl  Shift <Key>=:    larger-vt-font() \n\
                 Ctrl ~Shift <Key>-:    smaller-vt-font()

!!
